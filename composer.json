    },
    "autoload": {
        "psr-4": {
            "Killmails\\OAuth2\\Client\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "pest"
    }
}
